<launch>

	<group ns="camera">

	<group ns="front">
		<node name="image_proc" pkg="image_proc" type="image_proc"/>
	</group>

	<group ns="bottom">
		<node name="image_proc" pkg="image_proc" type="image_proc"/>
	</group>

	<node name="front" pkg="avt_vimba_camera" type="mono_camera_node" output="screen" args="-s">
		<param name="width" value="1360"/>
		<param name="height" value="1024"/>
	</node>

	<node name="bottom" pkg="avt_vimba_camera" type="mono_camera_node" output="screen" args="-s">
		<param name="width" value="1360"/>
		<param name="height" value="1024"/>
	</node>

	</group>
	
</launch>
